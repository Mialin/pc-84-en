			//删掉某个图片动作
			$('body').delegate('.fvpp-close','click',function(){
				$(this).parents(".uploaded-img").remove();
				var index = $(this).siblings('img').attr("index");	
			});
			
			//点击提交评论
			$("#comment-submit").click(function(){
				
					//获取消息框内容
					var msg = jQuery.trim($("textarea[name=msg]").val());
					
					//获取昵称
					var nickname = jQuery.trim($("input[name=nickname]").val());
					
					//获取商品评分
					var score = $("#score-callback-write input[name=score]").val();
					
					//获取商品id
					var gid = $("input[name=gid]").val();
					
					if(!score)
					{
						$.yamibuy.common.alert("Please enter a review");
							
						return false;
					}
					
					//如果检查输入都合法 则执行ajax
					if(checkInputContent(msg, nickname))
					{
						$.yamibuy.common.showLoading();
						$.yamibuy.common.doAjax(
		
							'goods.php',
							
							{
								act:"add_item_comment", 
								nickname:nickname, 
								content:msg, 
								gid:$("input[name=gid]").val(),
								score:score
							},
							
							function(data){
								//执行失败
								if(data.status == 1)
								{
									
									$.yamibuy.common.alert(data.message);	
								}
								//执行成功
								else if(data.status == 0)
								{
									window.location.href="goods.php?id="+gid;
								}
								else
								{
									$.yamibuy.common.alert(data.status);	
								}
							},
							
							
							{showloading:false}
						);
					}
					
					
			});
			
			//检查输入是否合法
			function checkInputContent(msg, nickname)
			{
				if(msg == "")
				{
					$.yamibuy.common.alert("Please enter a review");
					
					return false;
				}
				if(nickname == "")
				{
					$.yamibuy.common.alert("Please enter a nickname");	
					return false;
				}
				
				if(strLength(nickname) > 16)
				{
					$.yamibuy.common.alert("nickname can not exceed 16 characters or 8 Chinese characters");
					return false;
				}
				return true;
			}
			
			//获取字符串的长度 汉字算两个字符
			function strLength(str)
			{
				return str.replace(/[^\x00-\xff]/g,"**").length;
			}

			function oos_remind_me(gid)
			{
			
				$.yamibuy.user.displaySignIn({method : function(){
					
					$.yamibuy.common.doAjax(
	
						'goods.php',
						
						{
							act:"add_remind", 
							gid:gid
						},
						
						function(data){
							//执行失败
							if(data.status == 1)
							{
								$.yamibuy.common.alert(data.message);	
							}
							//执行成功
							else if(data.status == 0)
							{
								//$.yamibuy.common.showLoading();
								window.location.href="goods.php?id="+gid;
							}
							else
							{
								$.yamibuy.common.alert(data.status);	
							}
						},
						
						
						{showloading:false}
					);
				}});
				
			}
			
			function shareFb(gid,invite_code)
			{
				if(invite_code != "")
				{
					var invite_code = "&invite_code="+invite_code;
					
					FB.ui({
			        method: 'share',
			        href: 'http://www.yamibuy.com/en/goods.php?id='+gid+invite_code,
			    }, function(response){});
				}
				else
				{
					FB.ui({
			        method: 'share',
			        href: 'http://www.yamibuy.com/en/goods.php?id='+gid,
			    }, function(response){});
				}
			    
			}
			
			
			
