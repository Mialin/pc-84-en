		<script>
		function openPopup()
			{
				$.yamibuy.common.hideLoading();
				$('.popup-mask').css('display', 'block');
				$('html, body').css('overflow', 'hidden');
			}

			function closePopup()
			{
				$.yamibuy.common.hideLoading();
				$('.popup-mask').css('display', 'none');
				$('html, body').css('overflow', 'auto');
			}
			
			function sendMail()
			{
				toMail = $("#to").val();
				content = $("#msg").val();
				act = "sendMail";
				$.yamibuy.common.doAjax('goods.php',{act:act,toMail:toMail,content:content},
			        function(data)
			        {
			            if(data.status == 1)
			            {
			                $.yamibuy.common.alert('Mail Sent Failed');
			            }
			            else
			            {
			            	$("#to").val("");
			              closePopup(); 			              
			              $.yamibuy.common.alert('Mail Sent');		                          
			              
			            }
			        }
			    );
			}
			
			function inputZipcode(gid,in_stock)
			{
				$.yamibuy.common.zipcode("Please enter your zipcode:", function(){
					zipcode=$("#zipcode").val();
					if(zipcode != "")
					{
						setZipcode(gid,zipcode,in_stock);
					}
					else
					{
						$("#zipErr").empty().html('Please input your zipcode');
					}
				});
			}
			/*****驗證用戶是否輸入數字，非數字無法輸入*****/
			function ValidateNumber(e, pnumber)
			{
			    if (!/^\d+$/.test(pnumber))
			    {
			        $(e).val(/^\d+/.exec($(e).val()));
			    }
			    return false;
			}
			function setZipcode(gid,zipcode,in_stock)
			{
				$.yamibuy.common.doAjax('service.php',{act:"checkZipcode", gid:gid, zipcode:zipcode},
				function(data)
				{
					if(data.status == 1 )
					{
						$.yamibuy.common.alert(data.message);	
					}					
					else
					{	
						$("#setZipcode").show().html(zipcode);
						$("#inputZipcode").empty().html('Edit');
						
						if(data.object == 0)				
						{
							$("#notInArea").show();
							$(".add-to-cart").addClass("disabled-prim-btn");
							document.getElementById("btnText2").innerHTML = "This item can't be delivered to your area";
						}
						else
						{
							$("#notInArea").hide();
							
							$("#btnText").hide();
							$("#btnText2").show();
							$(".add-to-cart").removeClass("disabled-prim-btn");
							document.getElementById("btnText2").innerHTML = "<i class='fa fa-shopping-cart'></i>&nbsp;Add to cart";
						}
					}
				},{showloading:false});				
			}
			
		
	    $('[id=score-callback-read]').raty({
	            path: 'images',
	            half: true,
	            readOnly: true,
	            score: function() {
	            	return $(this).attr('data-score');
			    }
			});
			
			
			$('[id=score-callback-write]').raty({
	            path: 'images',
	            half: true,
	            readOnly: false,
	            score: function() {
	            	return $(this).attr('data-score');
			    }
			});
			
			
			
			var mainImage = $(".main-img");
			$(".thumb-cont img").hover(function(){
				$(".thumb-cont img").css("border-width", "0");
				$(this).css("border", "1px solid #f25950");

				var src = $(this).attr("src");
				$(".main-img").attr("src",src);
			});
			
			var files = null;
			
			$("#file").change(function(event){
				
				$(".uploaded-img-cont").hide();
				
				$(".uploaded-img").remove();

				files = event.currentTarget.files;
				
				var fileNum = files.length;
				
				var img = "";
				
				//如果存在上传的文件
				if(fileNum > 0)
				{
					for(var i=0; i<fileNum; i++)
					{
						var currentFile = files[i];	
						
						if(currentFile != undefined){
						
							var fileSize = currentFile.size;
							if(fileSize > 2000000)
							{
								alert("Image size needs be smaller than 2M");	
								$(this).val("");
								$(".uploaded-img").remove();
								return false;
							}
							var tmppath = URL.createObjectURL(currentFile);
							
							var img = img + '<div class="uploaded-img"><img index='+i+' id=img_'+i+' src='+tmppath+' width=50 height=50><a class="fvpp-close">&#10005;</a></div>';
						}
					}	
					
					
					$(".uploaded-img-cont").append(img);
				
					$("#submit-img").attr("disabled", false);
					
					$("#img-form" ).submit(function( event ) {
						event.preventDefault();
						var formData = new FormData();
						formData.append("act", "upload");
				        $.each($('#file')[0].files, function(i, file) {
				            formData.append('file-'+i, file);
				        });
		
				        
				        $.ajax({
							url: "service.php",
							data: formData,
							datatype: "POST",
							enctype: "multipart/form-data",
							contentType: false,
							processData: false,
							cache: false,
							success: function (data, textStatus, xhr) {
								alert("success");
							}
						});
					});
					$("#submit-img").click(function(){
						
						/*var formData = new FormData($("#img-form").serialize());
						
						$.ajax({
